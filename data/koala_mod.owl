  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Marvin">
    <owl:type rdf:resource="#Animal"/>
    <hasHabitat owl:resource="#UniLeipzig"/>
    <isHardWorking rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isHardWorking>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Jenna">
    <owl:type rdf:resource="#Person"/>
    <hasHabitat owl:resource="#UniLeipzig"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Felix">
    <owl:type rdf:resource="#Person"/>
    <hasHabitat owl:resource="#UniLeipzig"/>
    <isHardWorking rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isHardWorking>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Lisa">
    <owl:type rdf:resource="#Person"/>
    <hasHabitat owl:resource="#UniLeipzig"/>
    <isHardWorking rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isHardWorking>
  </owl:NamedIndividual>

  <!-- INDIVIDUALS FOR Parent -->

  <owl:NamedIndividual rdf:about="#Robert">
    <owl:type rdf:resource="#Person"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Philipp">
    <owl:type rdf:resource="#Elefant"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Julien">
    <owl:type rdf:resource="#Person"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Otto">
    <owl:type rdf:resource="#Animal"/>
    <hasHabitat rdf:resource="#ThatOneVeryDryEucalyptForest"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Kendrick">
    <owl:type rdf:resource="#Elefant"/>
    <hasChildren owl:resource="#Donna"/>
    <hasChildren owl:resource="#Nancy"/>
    <hasChildren owl:resource="#Carol"/>
  </owl:NamedIndividual>

  <!-- INDIVIDUALS FOR MaleStudentWith3Daughters -->

  <owl:NamedIndividual rdf:about="#Anna">
    <owl:type rdf:resource="#Student"/>
    <hasGender owl:Gender="female"/>
    <hasChildren owl:resource="#Donna"/>
    <hasChildren owl:resource="#Nancy"/>
    <hasChildren owl:resource="#Carol"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Linda">
    <owl:type rdf:resource="#Person"/>
    <isHardWorking rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isHardWorking>
    <hasChildren owl:resource="#Donna"/>
    <hasChildren owl:resource="#Nancy"/>
    <hasChildren owl:resource="#Carol"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#James">
    <owl:type rdf:resource="#Student"/>
    <hasChildren owl:resource="#Donna"/>
    <hasChildren owl:resource="#Nancy"/>
    <hasChildren owl:resource="#Carol"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#John">
    <owl:type rdf:resource="#Person"/>
    <hasHabitat owl:resource="#UniLeipzig"/>
    <hasChildren owl:resource="#Donna"/>
    <hasChildren owl:resource="#Nancy"/>
    <hasChildren owl:resource="#Carol"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Martin">
    <owl:type rdf:resource="#Student"/>
    <hasGender owl:Gender="male"/>
    <hasChildren owl:resource="#Donna"/>
    <hasChildren owl:resource="#Nancy"/>
    <hasChildren owl:resource="#Carol"/>
  </owl:NamedIndividual>

  <!-- GENERIC STUFF -->

  <owl:NamedIndividual rdf:ID="#Donna">
    <owl:type rdf:resource="#Female"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:ID="#Nancy">
    <owl:type rdf:resource="#Female"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:ID="#Carol">
    <owl:type rdf:resource="#Female"/>
  </owl:NamedIndividual>

  <owl:Class rdf:ID="Elefant">
    <owl:subClassOf>
      <owl:Class rdf:about="#Animal"/>
    </owl:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="City">
    <owl:subClassOf>
      <owl:Class rdf:about="#Habitat"/>
    </owl:subClassOf>
  </owl:Class>

  <owl:NamedIndividual rdf:about="#Leipzig">
    <owl:type rdf:resource="#City"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#UniLeipzig">
    <owl:type rdf:resource="#University"/>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#ThatOneVeryDryEucalyptForest">
    <owl:typee rdf:resource="#DryEucalyptForest"/>
